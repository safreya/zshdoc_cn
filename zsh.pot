#: zsh.texi:6673
#, no-wrap
msgid "[:space:]"
msgstr ""

#. type: table
#: zsh.texi:6675
msgid "The character is whitespace"
msgstr ""

#. type: t{#1}
#: zsh.texi:6676
#, no-wrap
msgid "[:upper:]"
msgstr ""

