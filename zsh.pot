#: zsh.texi:6666
msgid "The character is a lowercase letter"
msgstr ""

#. type: t{#1}
#: zsh.texi:6667
#, no-wrap
msgid "[:print:]"
msgstr ""

#. type: table
#: zsh.texi:6669
msgid "The character is printable"
msgstr ""

#. type: t{#1}
#: zsh.texi:6670
#, no-wrap
msgid "[:punct:]"
