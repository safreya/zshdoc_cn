msgid "up-line-or-history"
msgstr ""

#. type: item
#: zsh.texi:17992
#, no-wrap
msgid "@t{up-line-or-history} (@t{^P ESC-[A}) (@t{k}) (@t{ESC-[A})"
msgstr ""

#. type: table
#: zsh.texi:17995
