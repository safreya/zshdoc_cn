#: zsh.texi:6660
msgid "The character is a decimal digit"
msgstr ""

#. type: t{#1}
#: zsh.texi:6661
#, no-wrap
msgid "[:graph:]"
msgstr ""

#. type: table
#: zsh.texi:6663
msgid "The character is a printable character other than whitespace"
msgstr ""

#. type: t{#1}
#: zsh.texi:6664
#, no-wrap
msgid "[:lower:]"
msgstr ""

