msgstr ""

#. type: pindex
#: zsh.texi:9780
#, no-wrap
msgid "AUTOPUSHD"
msgstr ""

#. type: pindex
#: zsh.texi:9781
#, no-wrap
msgid "NOAUTOPUSHD"
msgstr ""

#. type: cindex
#: zsh.texi:9782
#, no-wrap
msgid "cd, behaving like pushd"
msgstr ""

#. type: cindex
#: zsh.texi:9783
#, no-wrap
msgid "pushd, making cd behave like"
msgstr ""

#. type: item
#: zsh.texi:9784
#, no-wrap
