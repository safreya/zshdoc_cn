msgstr "按位异或"

#. type: table
#: zsh.texi:3359 zsh.texi:3414
msgid "bitwise OR"
msgstr "按位或"

#. type: table
#: zsh.texi:3361 zsh.texi:3398
msgid "exponentiation"
msgstr "乘幂"

#. type: t{#1}
#: zsh.texi:3361 zsh.texi:3398
#, no-wrap
msgid "* / %"
msgstr "* / %"

#. type: table
#: zsh.texi:3363 zsh.texi:3400
