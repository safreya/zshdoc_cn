msgstr "三元运算符"

#. type: t{#1}
#: zsh.texi:3375 zsh.texi:3422
#, no-wrap
msgid "= += -= *= /= %= &= ^= |= <<= >>= &&= ||= ^^= **="
msgstr "= += -= *= /= %= &= ^= |= <<= >>= &&= ||= ^^= **="

#. type: cindex
#: zsh.texi:3377 zsh.texi:3424 zsh.texi:7761
#, no-wrap
msgid "assignment"
msgstr "赋值"

#. type: t{#1}
#: zsh.texi:3377 zsh.texi:3424
#, no-wrap
msgid ","
msgstr ","

