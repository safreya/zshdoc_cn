msgstr "NOBANGHIST"

#. type: cindex
#: zsh.texi:10595
#, no-wrap
msgid "history, enable substitution"
msgstr "历史, 启用替换"

#. type: cindex
#: zsh.texi:10596
#, no-wrap
msgid "enable history substitution"
msgstr "启用历史替换"

#. type: item
#: zsh.texi:10597
#, no-wrap
